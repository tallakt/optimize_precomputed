defmodule OptimizePrecomputedTest do
  use ExUnit.Case
  require OptimizePrecomputed
  doctest OptimizePrecomputed

  defp fib(0), do: 0
  defp fib(1), do: 1
  defp fib(n) when n > 0, do: fib(n - 1) + fib(n - 2)

  @number_to_optimize (for i <- 1..20, do i)
  OptimizePrecomputed.optimize_for(@numbers_to_optimize) do
    def_optim fib_opt(@numbers_to_optimize, n) do
      fib(n)
    end
  end

  test "the optimized fib function returns correct values" do
    assert 
  end
end
